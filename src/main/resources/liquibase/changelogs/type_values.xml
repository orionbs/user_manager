<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.4.xsd">

    <!-- Event Type Data -->
    <changeSet id="event-type-data" author="orionbs">

        <insert tableName="event_type">
            <column name="value" value="REGISTRATION"/>
        </insert>

        <insert tableName="event_type">
            <column name="value" value="AUTHENTICATION"/>
        </insert>

    </changeSet>

    <!-- Status Type Data -->
    <changeSet id="status-type-data" author="orionbs">

        <insert tableName="status_type">
            <column name="value" value="UNVERIFIED"/>
        </insert>

        <insert tableName="status_type">
            <column name="value" value="VERIFIED"/>
        </insert>

        <insert tableName="status_type">
            <column name="value" value="LOCKED"/>
        </insert>

    </changeSet>

</databaseChangeLog>